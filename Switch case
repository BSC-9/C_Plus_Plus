#include <iostream>
using namespace std;
class Date{
    public:
     int num;
};
class check : public Date{
    public:
    void day(){
        cin>>num;
    }
    void display(){
        switch(num){
            case 1:
            cout<<"Monday";
            break;
            case 2:
            cout<<"Tuesday";
            break;
            case 3:
            cout<<"Wednesday";
            break;
            case 4:
            cout<<"Thursday";
            break;
            case 5:
            cout<<"Friday";
            break;
            case 6:
            cout<<"Saturday";
            break;
            default:
            cout<<"Sunday";
            break;
        }
    }
};
int main()
{   check obj;
    obj.day();
    obj.display();
	return 0;
}
