#include <iostream>
using namespace std;

class library{
    public:
    int re,codee;
    string ne;
    library(int r,string n,int code){
      re=r;
      codee=code;
      ne=n;
    }
  void display(){
      cout<<"Roll No:"<<re<<endl
          <<"Name of the Student:"<<ne<<endl
          <<"Code of Book Accessed:"<<codee<<endl;
  }
};
int main()
{   int r1,r2,code1,code2;
    string n1,n2;
    cin>>r1>>n1>>code1;
    cin>>r2>>n2>>code2;
    library lib1(r1,n1,code1),lib2(r2,n2,code2);
    lib1.display();
    lib2.display();
	return 0;
}
